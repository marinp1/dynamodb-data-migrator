{
  "name": "dynamodb-migrator",
  "version": "0.1.0",
  "description": "Migrate DynamoDB tables",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/index.js",
    "test": "jest",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "dynamo-client": "DYNAMO_ENDPOINT=http://localhost:8000 node_modules/dynamodb-admin/bin/dynamodb-admin.js"
  },
  "author": "Patrik Marin",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.745.0",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.3",
    "@types/through2": "^2.0.36",
    "dynamodb-admin": "^4.0.0",
    "eslint-plugin-jest": "^24.0.0",
    "gts": "^2.0.2",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": "gts fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
