{
  "name": "dynamodb-migrator",
  "version": "0.1.0",
  "description": "Migrate DynamoDB tables",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "author": "Patrik Marin",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb-v2-node": "0.1.0-preview.2",
    "aws-sdk": "^2.745.0"
  },
  "devDependencies": {
    "@types/node": "^14.6.3",
    "gts": "^2.0.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": "gts fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
