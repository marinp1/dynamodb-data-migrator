{
  "name": "dynamodb-data-migrator",
  "version": "0.1.0",
  "description": "Migrate DynamoDB table content to another table",
  "main": "index.js",
  "scripts": {
    "compile": "rimraf ./build && tsc",
    "compile:init": "npm run build && node build/index.js initialize --ST migration-testing-table --SR eu-central-1 --SP patrikmarin",
    "build": "rollup -c --file ddm-v$npm_package_version.js",
    "test": "npm run check",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "dynamo-client": "DYNAMO_ENDPOINT=http://localhost:8000 node_modules/dynamodb-admin/bin/dynamodb-admin.js",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags && rm -rf build/temp"
  },
  "author": "Patrik Marin",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.745.0",
    "js-yaml": "^3.14.0",
    "lodash.chunk": "^4.2.0",
    "through2": "^4.0.2",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash.chunk": "^4.2.6",
    "@types/node": "^14.6.3",
    "@types/through2": "^2.0.36",
    "@types/yargs": "^15.0.5",
    "dynamodb-admin": "^4.0.0",
    "eslint-plugin-jest": "^24.0.0",
    "gts": "^2.0.2",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.11",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": "gts fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
